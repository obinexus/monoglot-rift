/**
 * @brief Initializes the accessibility manager with a configuration
 *
 * @param manager The manager to initialize
 * @param config The configuration to use, or NULL for default configuration
 * @return bool True if successful, false otherwise
 */
bool
rift_accessibility_manager_initialize(rift_accessibility_manager_t *manager,
                                      const rift_accessibility_config_t *config)
{
    if (!manager) {
        rift_set_error(RIFT_ERROR_INVALID_PARAMETER, "Invalid accessibility manager");
        return false;
    }

    /* Prevent double initialization */
    if (manager->initialized) {
        rift_set_error(RIFT_ERROR_ALREADY_INITIALIZED, "Accessibility manager already initialized");
        return false;
    }

    /* Use default configuration if none provided */
    rift_accessibility_config_t default_config;
    if (!config) {
        default_config = rift_accessibility_config_default();
        config = &default_config;
    }

    /* Create component managers */
    manager->color_scheme_manager = rift_color_scheme_manager_create();
    if (!manager->color_scheme_manager) {
        goto error_cleanup;
    }

    manager->keyboard_manager = rift_keyboard_navigation_manager_create();
    if (!manager->keyboard_manager) {
        goto error_cleanup;
    }

    manager->screen_reader = rift_screen_reader_adapter_create();
    if (!manager->screen_reader) {
        goto error_cleanup;
    }

    manager->ui_manager = rift_ui_adaptation_manager_create();
    if (!manager->ui_manager) {
        goto error_cleanup;
    }

    manager->preferences_manager = rift_preferences_manager_create();
    if (!manager->preferences_manager) {
        goto error_cleanup;
    }

    /* Initialize components with configuration */
    if (!rift_screen_reader_adapter_initialize(manager->screen_reader, config->screenReaderType)) {
        goto error_cleanup;
    }

    if (!rift_color_scheme_manager_set_active_scheme(manager->color_scheme_manager,
                                                     config->colorScheme)) {
        goto error_cleanup;
    }

    if (!rift_ui_adaptation_manager_apply_template(manager->ui_manager,
                                                   config->uiAdaptationTemplate)) {
        goto error_cleanup;
    }

    /* Register keyboard shortcuts */
    if (config->keyboardShortcuts && config->keyboardShortcutCount > 0) {
        for (size_t i = 0; i < config->keyboardShortcutCount; i++) {
            /* Note: we would need to register these with actual callbacks in a real
             * implementation
             */
            /* This is simplified for the example */
            if (!rift_keyboard_navigation_manager_register_shortcut(
                    manager->keyboard_manager, config->keyboardShortcuts[i].action,
                    config->keyboardShortcuts[i].key_binding, NULL, /* Callback would go here */
                    NULL /* User data would go here */)) {
                goto error_cleanup;
            }
        }
    }

    /* Set enabled features */
    manager->enabled_features = config->enabledFeatures;

    /* Set initialized flag */
    manager->initialized = true;

    return true;

error_cleanup:
    /* Clean up on failure */
    if (manager->color_scheme_manager) {
        rift_color_scheme_manager_destroy(manager->color_scheme_manager);
        manager->color_scheme_manager = NULL;
    }

    if (manager->keyboard_manager) {
        rift_keyboard_navigation_manager_destroy(manager->keyboard_manager);
        manager->keyboard_manager = NULL;
    }

    if (manager->screen_reader) {
        rift_screen_reader_adapter_destroy(manager->screen_reader);
        manager->screen_reader = NULL;
    }

    if (manager->ui_manager) {
        rift_ui_adaptation_manager_destroy(manager->ui_manager);
        manager->ui_manager = NULL;
    }

    if (manager->preferences_manager) {
        rift_preferences_manager_destroy(manager->preferences_manager);
        manager->preferences_manager = NULL;
    }

    return false;
}

