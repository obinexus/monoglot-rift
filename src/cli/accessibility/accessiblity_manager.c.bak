/**
 * @brief Disables specified accessibility features
 *
 * @param manager The accessibility manager
 * @param features Bitmask of features to disable
 * @return bool True if successful, false otherwise
 */
bool
rift_accessibility_manager_disable_features(rift_accessibility_manager_t *manager,
                                            rift_accessibility_feature_flags_t features)
{
    if (!manager) {
        rift_set_error(RIFT_ERROR_INVALID_PARAMETER, "Invalid accessibility manager");
        return false;
    }

    if (!manager->initialized) {
        rift_set_error(RIFT_ERROR_NOT_INITIALIZED, "Accessibility manager not initialized");
        return false;
    }

    /* Disable the specified features by AND-ing with the inverted bitmask */
    manager->enabled_features &= ~features;

    /* Apply changes based on newly disabled features */
    if (features & RIFT_ACCESSIBILITY_FEATURE_COLOR_SCHEME) {
        /* Revert to standard color scheme when this feature is disabled */
        rift_color_scheme_manager_set_active_scheme(manager->color_scheme_manager,
                                                    RIFT_COLOR_SCHEME_DEFAULT);
    }

    if (features & RIFT_ACCESSIBILITY_FEATURE_KEYBOARD_NAVIGATION) {
        /* Disable keyboard navigation when this feature is disabled */
        rift_keyboard_navigation_manager_clear_shortcuts(manager->keyboard_manager);
    }

    if (features & RIFT_ACCESSIBILITY_FEATURE_SCREEN_READER) {
        /* Clean up screen reader when this feature is disabled */
        rift_screen_reader_adapter_stop_speaking(manager->screen_reader);
    }

    if (features & RIFT_ACCESSIBILITY_FEATURE_UI_ADAPTATION) {
        /* Revert UI to default when this feature is disabled */
        rift_ui_adaptation_manager_apply_template(manager->ui_manager, RIFT_UI_TEMPLATE_DEFAULT);
    }

    return true;
}

