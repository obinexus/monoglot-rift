/**
 * @brief Creates a new accessibility manager
 *
 * @return rift_accessibility_manager_t* A new accessibility manager or NULL on failure
 */
rift_accessibility_manager_t *
rift_accessibility_manager_create(void)
{
    rift_accessibility_manager_t *manager =
        (rift_accessibility_manager_t *)rift_malloc(sizeof(rift_accessibility_manager_t));

    if (!manager) {
        rift_set_error(RIFT_ERROR_MEMORY_ALLOCATION,
                       "Failed to allocate memory for accessibility manager");
        return NULL;
    }

    /* Initialize structure */
    manager->color_scheme_manager = NULL;
    manager->keyboard_manager = NULL;
    manager->screen_reader = NULL;
    manager->ui_manager = NULL;
    manager->preferences_manager = NULL;
    manager->enabled_features = RIFT_ACCESSIBILITY_FEATURE_NONE;
    manager->initialized = false;

    return manager;
}

