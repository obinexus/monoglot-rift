/**
 * @brief Enables specified accessibility features
 *
 * @param manager The accessibility manager
 * @param features Bitmask of features to enable
 * @return bool True if successful, false otherwise
 */
bool
rift_accessibility_manager_enable_features(rift_accessibility_manager_t *manager,
                                           rift_accessibility_feature_flags_t features)
{
    if (!manager) {
        rift_set_error(RIFT_ERROR_INVALID_PARAMETER, "Invalid accessibility manager");
        return false;
    }

    if (!manager->initialized) {
        rift_set_error(RIFT_ERROR_NOT_INITIALIZED, "Accessibility manager not initialized");
        return false;
    }

    /* Enable the specified features by OR-ing with the current set */
    manager->enabled_features |= features;

    /* Apply changes based on newly enabled features */
    if (features & RIFT_ACCESSIBILITY_FEATURE_COLOR_SCHEME) {
        /* Apply default color scheme settings when this feature is first enabled */
        /* This would typically load from preferences or use a default */
    }

    if (features & RIFT_ACCESSIBILITY_FEATURE_KEYBOARD_NAVIGATION) {
        /* Set up keyboard navigation when this feature is first enabled */
        /* This would typically register standard shortcuts */
    }

    if (features & RIFT_ACCESSIBILITY_FEATURE_SCREEN_READER) {
        /* Initialize screen reader when this feature is first enabled */
        /* This would typically connect to the system screen reader */
    }

    if (features & RIFT_ACCESSIBILITY_FEATURE_UI_ADAPTATION) {
        /* Set up UI adaptation when this feature is first enabled */
        /* This would typically apply the current UI template */
    }

    return true;
}

